#include <stdio.h>

long seed, multi=40692, increase=127, module=10000000; /* valores default da semente, multiplicador, incremento e m√≥dulo */
long result; /* resultado do gerador */

long lcrandom ()
{
  result = ((multi*result) + increase)%module;
  return result;
}

void lcrandom_seed (long s)
{
  result = seed;
}

long lcrandom_max ()
{
  
}

void lcrandom_parms (long A, long C, long M)
{
  multi = A;
  increase = C;
  module = M;
}

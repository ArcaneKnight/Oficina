#include <stdio.h>
#include <stdlib.h>
#include <string.h>

char **dict;

int main ()
{
  FILE *arq;
  char *c, carac, *inD;
  int i;
  arq = fopen ("/usr/share/dict/brazilian", "r");
  if (! arq)
    {
      perror ("Erro ao abrir dicion√°rio.");
      exit (1);
    }
  dict = calloc (275502, sizeof(char*));
  c = NULL;
  for (i=0;i < 275502; i++)
    {
      fscanf (arq, "%[^\n]s", c);
      dict[i] = malloc (((unsigned)strlen(c)+1)*sizeof(char));
      strcpy (dict[i],c);
    }
  
  qsort (dict, 275502, 55*sizeof(char), (int(*)(const void*,const void*)) strcmp);
  
  scanf("%c", &carac);
  while (carac != EOF)
    {
      while ((carac < 65 || carac > 122 || (carac > 90 && carac < 97))  && (carac != EOF))
	{
	  putchar (carac);
	  scanf("%c", &carac);
	}

      c = NULL;;
      while (((carac >= 65 && carac <= 90) || (carac >= 97 && carac <= 122)) && (carac != EOF))
	{
	  c = c + carac;
	  scanf("%c", &carac);
	}
      
      if (c != NULL)
	{
	  inD = (char*)bsearch (c, dict, 275502, 55*sizeof(char), (int(*)(const void*,const void*)) strcmp);
	  if (inD != NULL)
	    puts(c);
	  else
	    printf ("[%s]", (char *)c);
	}
    }
  fclose(arq);
  free(dict);
  return 0;
}
